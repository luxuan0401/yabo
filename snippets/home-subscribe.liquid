<div class="homepage-page homepage--white">
	<div id="scrollSubscribe"></div>
  <div class="wrapper home-subscribe-container">
    <h3>Dear SavorMaster,</h3>
    <form class="" role="form" style="">
      <div class="banner-form-text">
		<span class="one-line">
			<span class="inline-block">
	        I am
	        <div class="banner-form-input">
	          <input class="dotted ng-pristine ng-invalid ng-invalid-required ng-touched" name="name" ng-model-options="{ allowInvalid: true }" ng-model="banner.name" placeholder="name" required="" type="text" style="">
	        </div>
			</span>
			<span class="inline-block">
	        and my email is
	        <div class="banner-form-input">
	          <input class="dotted ng-pristine ng-untouched ng-invalid ng-invalid-required ng-valid-email" name="email" ng-model-options="{ allowInvalid: true }" ng-model="banner.email" placeholder="email" required="" type="email">
	        </div>
			</span>
		</span>
        <span class="one-line">
          I want to join your project and tell you what I want to eat.
        </span>
		<span class="one-line">
        I am
        <div class="banner-form-input">
          <input class="dotted ng-pristine ng-invalid ng-invalid-required ng-touched" name="name" ng-model-options="{ allowInvalid: true }" ng-model="banner.name" placeholder="name" required="" type="text" style="">
        </div>
		</span>
		 <span class="one-line">
	          I'd like to subscribe for free examples.
	        </span>
      </div>
      <!-- ngIf: banner.bannerForm.$submitted && !banner.bannerForm.$valid -->
      <a class="global-button" href="#">
        Subscribe
      </a>
    </form>
  </div>
</div>